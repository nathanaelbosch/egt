#!/usr/bin/bash

python -m egt.main \
       --test-function double_global_ackley \
       --max-iterations 1000000 \
       --gamma 0.05 \
       --stepsize 0.5 \
       --n-points 50 \
       --plot-range -100 100 \
       --point-interval -100 100 \
       --initial-strategy standard \
       --max-animation-seconds 10 \
       --epsilon 1e-4 \
       --normalize-delta \
       --U-interval -10 10 \
       --beta 30 \
       --n-strategies 50 \
       --save-prefix double_global \
